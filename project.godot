config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="godot-game"
run/main_scene="res://scenes/MainMenu.tscn"
boot_splash/image="res://assets/textures/empty.png"
boot_splash/bg_color=Color( 0.152941, 0.156863, 0.133333, 1 )
config/icon="res://icon.png"

[autoload]

Audio="*res://scenes/Audio.tscn"
SavedData="*res://scenes/SavedData.tscn"
SceneChanger="*res://scenes/SceneChanger.tscn"

[debug]

gdscript/warnings/enable=false

[display]

window/size/width=676
window/size/height=896
window/dpi/allow_hidpi=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

theme/custom="res://themes/default_theme.tres"

